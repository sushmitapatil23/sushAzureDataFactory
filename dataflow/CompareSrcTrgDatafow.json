{
	"name": "CompareSrcTrgDatafow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_txtTableA",
						"type": "DatasetReference"
					},
					"name": "TxtTableAsource"
				},
				{
					"dataset": {
						"referenceName": "ds_TableA",
						"type": "DatasetReference"
					},
					"name": "Table1source"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "HashColSource1"
				},
				{
					"name": "HashColSource2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {101} as string,",
				"          John as string,",
				"          {1500} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> TxtTableAsource",
				"source(output(",
				"          id as string,",
				"          name as string,",
				"          salary as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Table1source",
				"TxtTableAsource derive(HashColSource1 = sha2(256, columns())) ~> HashColSource1",
				"Table1source derive(HasColSource2 = sha2(256, columns())) ~> HashColSource2"
			]
		}
	}
}